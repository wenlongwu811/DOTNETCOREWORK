.top-message {width: 100%; height: 28px; line-height: 28px; text-align: center; font-size: 14px; color: #fff; background-color: rgba(0, 0, 0, 0.25); position: fixed; top: 0; left: 0;}
.top-message a {color: #fff;}
.hide {display: none;}
